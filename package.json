{
  "name": "tint",
  "version": "0.1.0",
  "description": "Open-source menu bar (status menu) color picker for OSX.",
  "main": "src/main.js",
  "scripts": {
    "build": "electron-packager . Tint --overwrite --platform=darwin --arch=x64 --version=0.35.0 --icon=src/images/Tint.icns --ignore='node_modules/(electron-packager|electron-prebuilt|babel-cli|babel-plugin-transform-react-jsx|babel-preset-es2015|babel-plugin-syntax-jsx)'",
    "transpile": "babel -o src/build.js src/web.js",
    "start": "npm run transpile && electron src/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danawoodman/tint.git"
  },
  "keywords": [
    "color",
    "picker",
    "color",
    "tint",
    "menu",
    "bar",
    "status",
    "bar",
    "menu",
    "status",
    "picker",
    "hex",
    "rgb",
    "rgba",
    "utility",
    "osx",
    "app"
  ],
  "author": "Dana Woodman <dana@danawoodman.com> (http://danawoodman.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danawoodman/tint/issues"
  },
  "homepage": "https://github.com/danawoodman/tint#readme",
  "devDependencies": {
    "babel-cli": "6.1.18",
    "babel-plugin-syntax-jsx": "6.1.18",
    "babel-plugin-transform-react-jsx": "6.1.18",
    "babel-preset-es2015": "6.1.18",
    "electron-packager": "5.1.1",
    "electron-prebuilt": "0.35.0"
  },
  "dependencies": {
    "electron-positioner": "2.0.2",
    "extend": "3.0.0",
    "react": "0.14.2",
    "react-color": "1.2.1",
    "react-dom": "0.14.2"
  }
}
