{
  "name": "tint",
  "version": "1.0.0",
  "description": "Open-source menu bar (status menu) color picker for OSX.",
  "main": "src/boot.js",
  "scripts": {
    "build": "electron-packager . Tint --platform=darwin --arch=x64 --version=0.35.0 --icon=Tint.icns --ignore='node_modules/(electron-packager|electron-prebuilt|electron-compilers|electron-compile)'",
    "start": "electron src/boot.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danawoodman/tint.git"
  },
  "keywords": [
    "color",
    "picker",
    "color",
    "tint",
    "menu",
    "bar",
    "status",
    "bar",
    "menu",
    "status",
    "picker",
    "hex",
    "rgb",
    "rgba",
    "utility",
    "osx",
    "app"
  ],
  "author": "Dana Woodman <dana@danawoodman.com> (http://danawoodman.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danawoodman/tint/issues"
  },
  "homepage": "https://github.com/danawoodman/tint#readme",
  "devDependencies": {
    "electron-compilers": "1.0.1",
    "electron-packager": "5.1.1",
    "electron-prebuilt": "0.35.0"
  },
  "dependencies": {
    "electron-compile": "1.0.1",
    "menubar": "2.3.0",
    "react": "0.14.2",
    "react-color": "1.2.1",
    "react-dom": "0.14.2"
  }
}
