{
  "name": "tint",
  "version": "2.1.0",
  "description": "Menu bar (status menu) color picker for OSX. Updated & modified by A Daring Design",
  "main": "src/main.js",
  "scripts": {
    "build": "electron src/main.js",
    "package": "electron-packager . Tint --overwrite --platform=darwin --arch=x64 --electron-version=1.6.6 --icon=src/images/Tint.icns --ignore='node_modules/(babel-cli|babel-plugin-transform-react-jsx|babel-preset-es2015|babel-plugin-syntax-jsx|babel-.*)'",
    "start_babel": "npm run transpile && npm run build",
    "transpile": "babel -o src/build.js src/web.js",
    "start": "electron src/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adaringdesign/tint.git"
  },
  "keywords": [
    "color",
    "picker",
    "color",
    "tint",
    "menu",
    "bar",
    "status",
    "bar",
    "menu",
    "status",
    "picker",
    "hex",
    "rgb",
    "rgba",
    "utility",
    "osx",
    "app"
  ],
  "author": "Rutger Valk-van de Klunder - A Daring Design <info@adaringdesign.nl> (https://www.adaringdesign.nl/).",
  "license": "ISC",
  "bugs": {
    "url": "https://www.adaringdesign.nl/"
  },
  "homepage": "https://www.adaringdesign.nl/",
  "devDependencies": {
    "isdev": "^1.0.1",
    "babel-cli": "6.1.18",
    "babel-plugin-syntax-jsx": "6.1.18",
    "babel-plugin-transform-react-jsx": "6.1.18",
    "babel-preset-es2015": "6.1.18"
  },
  "dependencies": {
    "auto-launch": "^5.0.1",
    "electron": "^1.6.6",
    "electron-lets-move": "0.0.5",
    "electron-localshortcut": "^1.1.1",
    "electron-positioner": "^3.0.0",
    "electron-settings": "^3.0.14",
    "jquery": "^3.2.1"
  }
}
